/* pages/component/login/login.wxss */

.login-container {
  width: 100%;
  position: fixed;
  top:0;
  right: -100%;
  padding-top: 194.22rpx;
  height: 100%;

}

.login-is-container {
  right: -100%;
  animation: mylive 0.5s ease both;
  z-index: 1000;
  background-color: white;
}

.login-no-container {
  right: 0;
  z-index: 100;
  animation: mynolive 0.5s ease both;
  background-color: white;
}

@keyframes mylive {
  form {
    right: -100%;
  }

  to {
    right: 0;
  }
}

@keyframes mynolive {
  form {
    right: 0;
  }

  to {
    right: -100%;
  }
}

.login-body {
  width: 100%;
  text-align: center;
  padding: 0 77.22rpx 191.88rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  flex-direction: column;
  background:#fff;
}

.login-img {
  width: 240rpx;
  height: 212rpx;
  margin: 0 auto 51.48rpx;
  background-size: cover;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center;
}

.login-text {
  font-size: 37.44rpx;
  color: #1081FF;
  height: 58.5rpx;
  line-height: 58.5rpx;
  margin-bottom: 170.82rpx;
}

.login-button {
  border-radius: 1170rpx;
  width: 592.02rpx;
  height: 88.92rpx;
  line-height: 88.92rpx;
  color: white;
  font-size: 32.76rpx;
}

.login-uplogin {
  background-color: #1081FF;
}

.login-nologin {
  background-color: #73B5FF;
  margin-top: 39.78rpx;
}
