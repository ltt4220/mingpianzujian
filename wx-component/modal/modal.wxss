
.mark{
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .5);
  overflow: hidden;
  display: flex;
  display:-webkit-flex;
  justify-content: center;
  align-items: center;
  z-index: 1000;
}
