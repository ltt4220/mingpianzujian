}
.btn{
  width:100%;
  height: 98.28rpx;
  overflow: hidden;
}
.btn .l,.r{
  display: inline-block;
  width: 50%;
  box-sizing: border-box;
  font-size: 32.76rpx;
  height: 100%;
  line-height:98.28rpx;
  text-align: center;
}
.btn .l{
  border-right: solid 2.34rpx #F2F2F2;
}
