.contnet{
  box-sizing: border-box;
  height: 219.96rpx;
  border-bottom: solid 1px #F2F2F2;
  /* padding: 79.56rpx 46.8rpx 0rpx 46.8rpx; */
   padding: 0rpx 46.8rpx 0rpx 46.8rpx;
